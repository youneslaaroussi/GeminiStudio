# Google Cloud / Firebase
GOOGLE_PROJECT_ID=your_project_id
FIREBASE_SERVICE_ACCOUNT_KEY=/path/to/service-account.json

# Redis
REDIS_URL=redis://localhost:6379/0

# Server
APP_HOST=0.0.0.0
APP_PORT=8082
DEBUG=false
