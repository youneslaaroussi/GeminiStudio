{
  "name": "@gemini-studio/renderer",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && pnpm run build:headless && tsc --project tsconfig.build.json && pnpm run copy:headless",
    "copy:headless": "cp -r headless/dist dist/headless",
    "build:headless": "tsx scripts/build-headless.ts",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^5.2.2",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/montserrat": "^5.2.8",
    "@fontsource/poppins": "^5.2.7",
    "@fontsource/roboto": "^5.2.9",
    "@motion-canvas/core": "^3.17.2",
    "bullmq": "^5.26.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "get-port": "^7.1.0",
    "ioredis": "^5.4.1",
    "pino": "^9.4.0",
    "puppeteer": "^22.15.0",
    "puppeteer-cluster": "^0.24.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "uuid": "^10.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.16.10",
    "@types/socket.io": "^3.0.2",
    "esbuild": "^0.23.1",
    "pino-pretty": "^11.2.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.1"
  }
}
